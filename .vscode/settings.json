{
    "editor.tabSize": 8,
    "editor.indentSize": 4,
    "editor.insertSpaces": true,
"editor.detectIndentation": false,
"files.associations": {
        "atomic": "c",
        "*.tcc": "c",
        "istream": "c",
        "ostream": "c"
},
"files.exclude": {
    "**/*.a": true,
    "**/*.o": true,
    "src/.deps": true,
}
}
